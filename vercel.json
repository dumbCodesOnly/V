{
  "version": 2,
  "rewrites": [
    {
      "source": "/webhook",
      "destination": "/api/app.py"
    },
    {
      "source": "/webhook/toobit",
      "destination": "/api/app.py"
    },
    {
      "source": "/api/exchange/(.*)",
      "destination": "/api/app.py"
    },
    {
      "source": "/(.*)",
      "destination": "/api/app.py"
    }
  ],
  "functions": {
    "api/app.py": {
      "maxDuration": 60
    }
  },
  "env": {
    "VERCEL": "1"
  }
}